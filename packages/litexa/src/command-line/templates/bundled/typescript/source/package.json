{
  "name": "{name}",
  "version": "1.0.0",
  "scripts": {
    "clean": "npx --ignore-existing rimraf node_modules litexa/main.js && npm install",
    "compile": "npx webpack --config webpack.config.js && npx tsc",
    "compile:watch": "npx webpack --config webpack.config.js --watch",
    "deploy": "npm run compile && litexa deploy",
    "lint": "npx tslint -c tslint.json --project tsconfig.json --fix",
    "test": "npx mocha './test/**/*.spec.ts' && npm run test:litexa",
    "test:file": "npx mocha",
    "test:litexa": "npm run compile && litexa test",
    "test:litexa:watch": "npm run compile:watch | litexa test -w",
    "test:watch": "npx mocha './test/**/*.spec.ts' --watch"
  },
  "author": "Amazon",
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/mocha": "5.2.7",
    "@types/node": "13.1.8",
    "@types/pino": "5.15.3",
    "@types/sinon": "7.5.1",
    "chai": "4.2.0",
    "mocha": "7.0.0",
    "sinon": "8.1.0",
    "source-map-support": "0.5.16",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "pino": "5.16.0",
    "pino-pretty": "3.5.0"
  },
  "license": "ISC"
}
