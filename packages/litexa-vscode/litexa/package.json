{
  "name": "litexa-syntax-highlighting",
  "displayName": "Litexa Syntax Highlighting",
  "version": "0.0.1",
  "publisher": "Amazon",
  "engines": {
    "vscode": "^1.38.0"
  },
  "license": "Apache-2.0",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "litexa",
        "aliases": [
          "Litexa",
          "litexa"
        ],
        "extensions": [
          ".litexa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "litexa",
        "scopeName": "source.litexa",
        "path": "./syntaxes/litexa.tmLanguage.json"
      }
    ]
  }
}
