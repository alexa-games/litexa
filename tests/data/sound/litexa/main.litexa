launch

playFile
  playMusic sound.mp3

playURL
  playMusic "https://www.example.com/sound.mp3"

playSfx
  soundEffect sound.mp3
    or sound.mp3

stop
  stopMusic

global
  when "play file"
    -> playFile
  when "play url"
    -> playURL
  when "play random mp3"
    -> playSfx
  when "stop"
    -> stop
