{
  "manifest": {
    "manifestVersion": "1.0",
    "publishingInformation": {
      "isAvailableWorldwide": false,
      "distributionCountries": [
        "US"
      ],
      "distributionMode": "PUBLIC",
      "category": "GAMES",
      "testingInstructions": "replace with testing instructions",
      "gadgetSupport": {
        "requirement": "REQUIRED",
        "numPlayersMin": 1,
        "numPlayersMax": 1,
        "minGadgetButtons": 1,
        "maxGadgetButtons": 4
      },
      "locales": {
        "en-US": {
          "name": "Gadgets Api (default)",
          "summary": "replace with brief description, no longer than 120 characters",
          "description": "Longer description, goes to the skill store. \n\nLine breaks are supported.",
          "examplePhrases": [
            "Alexa, launch Gadgets Api",
            "Alexa, open Gadgets Api",
            "Alexa, play Gadgets Api"
          ],
          "keywords": [
            "game",
            "fun",
            "single player",
            "modify this list as appropriate"
          ],
          "smallIconUri": "https://s3.us-east-1.amazonaws.com/suncoast-assets/gadgets-api/default/default/icon-108.png",
          "largeIconUri": "https://s3.us-east-1.amazonaws.com/suncoast-assets/gadgets-api/default/default/icon-512.png"
        }
      }
    },
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "usesPersonalInfo": false,
      "isChildDirected": false,
      "isExportCompliant": true,
      "containsAds": false,
      "locales": {
        "en-US": {
          "privacyPolicyUrl": "http://yoursite/privacy.html",
          "termsOfUseUrl": "http://yoursite/terms.html"
        }
      }
    },
    "apis": {
      "custom": {
        "endpoint": {
          "uri": "arn:aws:lambda:us-east-1:085202703559:function:gadgets-api_default_litexa_handler:release-0"
        },
        "regions": {
          "NA": {
            "endpoint": {
              "uri": "arn:aws:lambda:us-east-1:085202703559:function:gadgets-api_default_litexa_handler:release-0"
            }
          }
        },
        "interfaces": [
          {
            "type": "GADGET_CONTROLLER"
          },
          {
            "type": "GAME_ENGINE"
          }
        ]
      }
    }
  }
}